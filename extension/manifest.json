{
	"manifest_version": 3,
	"name": "NoRick: Banishing A God",
	"version": "1.0.0",
	"description": "uBlock 4 rickrolls",
	"author": "Cuboid Raptor",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"src/jquery.js",
				"src/content.js"
			],
			"run_at": "document_start"
		}
	],
	"action": {
		"icons": {
			"128": "html/img/icon.png"
		},
		"default_title": "NoRick: Banishing A God",
		"default_popup": "html/popup.html"
	},
	"background": {
		"service_worker": "src/background.js"
	},
	"permissions": [
		"storage"
	],
	"icons": {
		"128": "html/img/icon.png"
	}
}